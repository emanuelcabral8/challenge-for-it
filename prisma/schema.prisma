generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Task {
  id          String   @id @default(uuid()) @db.Uuid
  title       String
  description String
  completed   Boolean
  created_at  DateTime @default(now())
   @@map("task")
}
  